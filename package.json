{
  "name": "swig-ts-sdk-dapp-examples",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:wallet-standard": "turbo run dev --filter=@swig/wallet-standard-dapp",
    "dev:legacy-dapp": "turbo run dev --filter=@swig/legacy-dapp-example",
    "dev:swig-native": "turbo run dev --filter=@swig/swig-native-dapp",
    "dev:in-app-wallet": "turbo run dev --filter=@swig/in-app-wallet-example",
    "dev:para-modal": "turbo run dev --filter=@swig/para-modal-example",
    "dev:malicious-dapp": "turbo run dev --filter=@swig/malicious-dapp",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@types/chrome": "^0.0.309",
    "prettier": "^2.8.8",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "turbo": "^2.5.3",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "yarn@4.9.2",
  "dependencies": {
    "@noble/curves": "^1.9.1",
    "@solana/spl-token": "^0.4.13",
    "@solana/spl-token-registry": "^0.2.4574",
    "@swig-wallet/classic": "1.1.0",
    "react-icons": "^5.5.0"
  },
  "resolutions": {
    "@swig-wallet/coder": "1.1.0"
  }
}
